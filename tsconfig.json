{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es2016",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "types": [ ],
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "*": [ "node_modules/*"]
    }
  },
  "exclude": [
    "src/**/*.test.ts"
  ]
}
